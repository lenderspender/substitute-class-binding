{
    "name": "lenderspender/substitute-class-binding",
    "description": "Add substitute class bindings to Laravel",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Bert van Hoekelen",
            "email": "bert@magwel.nl"
        }
    ],
    "autoload": {
        "psr-4": {
            "LenderSpender\\SubstituteClassBinding\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LenderSpender\\SubstituteClassBinding\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^7.0",
        "lenderspender/php-cs-fixer-rules": "dev-master",
        "phpstan/phpstan": "^1.3.2"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "analyse": "vendor/bin/phpstan analyse",
        "fix": "vendor/bin/php-cs-fixer fix"
    }
}
